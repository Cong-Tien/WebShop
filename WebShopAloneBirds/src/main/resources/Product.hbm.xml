<hibernate-mapping>
  <class name="com.dht.pojo.Product" table="product">
      <id name="id" column="id" type="int" />
      <property name="name" column="name" type="string" />
      <property name="price" column="price" type="int" />
      <property name="image" column="image" type="string" />
      <property name="image_ad" column="image_ad" type="string" />
      <property name="category_id" column="category_id" type="int" />
  </class>
  
  <class name="com.dht.pojo.Category" table="category">
      <id name="id" column="id" type="int" />
      <property name="namec" column="namec" type="string" />
      
  </class>
  <class name="com.dht.pojo.Account" table="account">
      <id name="user" column="user" type="string" />
      <property name="pass" column="pass" type="string" />
      
  </class>
</hibernate-mapping>
